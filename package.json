{
  "name": "youtube-speed-memory",
  "description": "A web extension to remember your per-video playback speed preferences for YouTube",
  "version": "2.0.0",
  "author": "Andrew Brey",
  "private": false,
  "repository": "https://github.com/andrewbrey/youtube-speed-memory",
  "bugs": "https://github.com/andrewbrey/youtube-speed-memory/issues",
  "scripts": {
    "build:dev:chrome": "NODE_ENV=development TARGET=chrome webpack --config ./webpack/webpack-config.js --watch",
    "build:dev:firefox": "NODE_ENV=development TARGET=firefox webpack --config ./webpack/webpack-config.js --watch",
    "build:prod:chrome": "NODE_ENV=production TARGET=chrome webpack --config ./webpack/webpack-config.js",
    "build:prod:firefox": "NODE_ENV=production TARGET=firefox webpack --config ./webpack/webpack-config.js",
    "lint:chrome": "web-ext lint --source-dir ./extensions/chrome/",
    "lint:firefox": "web-ext lint --source-dir ./extensions/firefox/",
    "package:chrome": "web-ext build --source-dir ./extensions/chrome/ -a ./extensions/chrome/",
    "package:firefox": "web-ext build --source-dir ./extensions/firefox/ -a ./extensions/firefox/",
    "build": "npm run build:prod:chrome && npm run lint:chrome && npm run package:chrome && npm run build:prod:firefox && npm run lint:firefox && npm run package:firefox"
  },
  "dependencies": {
    "webextension-polyfill-ts": "0.10.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@types/node": "12.11.7",
    "@types/webpack-env": "1.14.1",
    "autoprefixer": "9.7.0",
    "chalk": "2.4.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "3.2.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "postcss-custom-properties": "9.0.2",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nesting": "7.0.1",
    "prettier": "1.18.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.0",
    "tailwindcss": "1.1.3",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.4",
    "web-ext": "3.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-fix-style-only-entries": "0.4.0",
    "webpack-merge": "4.2.2",
    "zip-webpack-plugin": "3.0.0"
  },
  "keywords": [
    "youtube",
    "chrome-extension",
    "browser-extension",
    "playback-speed",
    "memory"
  ],
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0",
    "yarn": ">=1.19.1"
  },
  "license": "MIT"
}
